{{#if isLoading}}
  {{three-bounce-spinner}}
{{else}}
  <div class="content-distribution-by-resource pull-left">
    {{charts.double-layer-donut-chart defaultI18nLabel='activities.resources-available' radius=130 innerRadius=120 radius1=110 innerRadius1=100 data=resources}}
  </div>

  <div class="content-distribution-by-question pull-left">
    {{charts.double-layer-donut-chart defaultI18nLabel='common.questions' radius=110 innerRadius=100 radius1=90 innerRadius1=80 data=questions}}
  </div>
  <div class="clear"></div>
{{/if}}
