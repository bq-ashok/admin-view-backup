<svg  width="{{width}}" height="{{height}}" viewport="0 0 {{width}} {{height}}" version="1.1" xmlns="http://www.w3.org/2000/svg">
  <circle r="{{radius}}" cx="{{xRadius}}" cy="{{yRadius}}" fill="transparent" stroke-dasharray="{{dataOffset}}" stroke-dashoffset="0"></circle>
  <circle id="bar" r="{{radius}}" cx="{{xRadius}}" cy="{{yRadius}}" fill="transparent" stroke-dasharray="{{dataOffset}}" stroke-dashoffset="0"></circle>
  <text x={{xRadius}} y={{yText}}>{{value}}</text>
</svg>
